var redraw = function () {
   // Populate the data table.
    var dataTable = google.visualization.arrayToDataTable([
        ['Mon',<%= rand*100 %>, <%= rand*100 %>, <%= rand*100 %>, <%= rand*100 %>],
       ['Tues',31,38,55,66],
       ['Wed',50,55,77,80],
       ['Thurs',77,77,66,50],
       ['Fri',68,66,22,15],
    ], true);
    // Draw the chart.
    var chart = new google.visualization.CandlestickChart(document.getElementById('chart_goes_here'));
    chart.draw(dataTable, {legend:'none', width:600, height:400});
};

redraw();
